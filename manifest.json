{
  "name": "Tuenti Imagenes",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Descarga facilmente imagenes de tuenti.com",
  "permissions": [ 
    "contextMenus"
  ],
  "background": {
    "scripts": ["tuenti-img.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://www.tuenti.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
